
<% if @current_user %>
  <h1>New Place</h1>

  <%= form_with :model => @place do |form| %>
    <p>
      <%= form.label "name" %>
      <%= form.text_field "name" %>
    </p>
    <p>
      <%= form.submit %>
    </p>
  <% end %>
<% else %>
<a> Please create an account and login to see the content </a>       
<% end %>